<?xml version="1.0"?>
<?xml-stylesheet href="chrome://palemon_barebones_tab_groups/skin/skin.css"
	type="text/css"?>
<!DOCTYPE overlay
	SYSTEM "chrome://palemon_barebones_tab_groups/locale/browser.dtd">
<overlay id="sample" 
	xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">
	<script src="main.js" />

	<menupopup id="menu_ToolsPopup">
		<menuitem label="&open_barebones_tab_groups.label;"
			key="palemon_barebones_tab_groups-run-key"
			oncommand="main()"/>
	</menupopup>

	<!-- Keyboard shortcut... -->
	<keyset>
		<key id="palemon_barebones_tab_groups-run-key"
			modifiers="alt shift" key="L"
			oncommand="main()"/>
	</keyset>

