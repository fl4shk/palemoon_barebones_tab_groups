	<toolbarpalette id="BrowserToolbarPalette">
		<toolbaritem>
			<toolbarbutton id="palemon_barebones_tab_groups-toolbar-button"
				label="Pale Moon Barebones Tab Groups"
				class="toolbarbutton-1 chromeclass-toolbar-additional"
				tooltiptext="&open_barebones_tab_groups.label;"
				oncommand="main()"/>
			<menulist label="List of Binops">
				<menupopup id="binop_menu">
					<menuitem label="Add"/>
					<menuitem label="Subtract"/>
					<menuitem label="Multiply"/>
					<menuitem label="Divide"/>
